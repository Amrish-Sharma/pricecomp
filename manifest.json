{
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "css": [
                "price-watch.css"
            ],
            "js": [
                "jquery-3.6.0.min.js",
                "popup.js"
            ],
            "matches": [
                "https://www.flipkart.com/*"
            ],
            "run_at": "document_end"
        }
    ],
    "description": "Compare prices of products on Flipkart and Amazon",
    "manifest_version": 2,
    "name": "PriceComp",
    "permissions": [
        "tabs",
        "https://www.flipkart.com/*",
        "https://amazon.in/*"
    ],
    "version": "1.0"
}